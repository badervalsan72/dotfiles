execute pathogen#infect()
syntax on 
filetype plugin indent on 
syntax on 
:command WQ wq
:command Wq wq 
:command Q q 
:command W w 
:command NT NERDTreeToggle
:set number
:set background=dark
:set termguicolors
let ayucolor='dark'
:colorscheme ayu 
set expandtab 
set tabstop=4 
set softtabstop=4
set shiftwidth=4
set autoindent 
